{
  "name": "@apiquest/plugin-auth-ui",
  "version": "1.0.0",
  "description": "Auth UI Plugin for ApiQuest Desktop",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "dev": "vite build --watch"
  },
  "apiquest": {
    "type": "auth-ui",
    "runtime": [
      "desktop"
    ],
    "capabilities": {
      "provides": {
        "authTypes": [
          "bearer",
          "basic",
          "apikey",
          "oauth2"
        ]
      },
      "supports": {
        "protocols": [
          "http",
          "graphql",
          "grpc"
        ]
      }
    }
  },
  "dependencies": {
    "@apiquest/plugin-auth": "^1.0.0"
  },
  "peerDependencies": {
    "@apiquest/plugin-ui-types": "workspace:*",
    "@apiquest/types": "^1.0.0",
    "@heroicons/react": "^2",
    "@radix-ui/colors": "^3",
    "@radix-ui/themes": "^3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@apiquest/plugin-ui-types": "workspace:*",
    "@apiquest/types": "^1.0.0",
    "@types/react": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
